<mx:Object 
	xmlns:mx="http://www.adobe.com/2006/mxml"
	xmlns:tag="org.spicefactory.parsley.tag.core.*"
	xmlns:msg="org.spicefactory.parsley.tag.messaging.*"
	xmlns:test="org.spicefactory.parsley.testmodel.*"
	xmlns:model="org.spicefactory.parsley.core.decorator.injection.*">
	
	<mx:Script>
		<![CDATA[
			import org.spicefactory.parsley.flex.mxmlconfig.core.CoreModel;		
			import org.spicefactory.parsley.flex.view.Panel3;
			import org.spicefactory.parsley.flex.view.module.model.*;		
			import flash.events.Event;
		]]>
	</mx:Script>
	
	<tag:MxmlViewTag type="{Panel3}">
	    <tag:PropertyTag name="model" idRef="child"/>
	</tag:MxmlViewTag>
	
	<tag:MxmlRootObjectTag type="{ClassNotCompiledIntoRootApp}">
		<tag:PropertyTag name="dependency" idRef="mdep"/>
		<msg:MessageHandlerDecorator method="onAnything" selector="anything" type="{Event}"/>
	</tag:MxmlRootObjectTag>
	
	<tag:MxmlRootObjectTag id="mdep" type="{ModuleDependency}"/>
	
	<tag:MxmlRootObjectTag id="child" type="{CoreModel}">
		<tag:ConstructorTag>
			<mx:String>module</mx:String>
			<mx:int>9</mx:int>
		</tag:ConstructorTag>
		<tag:PropertyTag name="booleanProp" value="true"/>
		<tag:PropertyTag name="refProp" idRef="dependency"/>
		<tag:PropertyTag name="arrayProp">
			<tag:ArrayTag>
				<mx:String>AA</mx:String>
				<mx:String>BB</mx:String>
				<tag:ObjectReferenceTag idRef="dependency"/>
			</tag:ArrayTag>
		</tag:PropertyTag>
	</tag:MxmlRootObjectTag> 
	
	
</mx:Object> 