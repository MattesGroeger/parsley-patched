<mx:Object 
	xmlns:mx="http://www.adobe.com/2006/mxml"
	xmlns:dec="org.spicefactory.parsley.tag.lifecycle.*"
	xmlns:flextag="org.spicefactory.parsley.tag.core.*">
	
	<mx:Script>
		<![CDATA[
			import org.spicefactory.parsley.core.decorator.asyncinit.model.*;		
		]]>
	</mx:Script>
	
	
	<flextag:MxmlObjectDefinitionFactoryTag id="asyncInitModel1" type="{AsyncInitModel}">
		<dec:AsyncInitDecorator order="1"/>
	</flextag:MxmlObjectDefinitionFactoryTag> 
	
	<flextag:MxmlObjectDefinitionFactoryTag id="asyncInitModel2" type="{AsyncInitModel}">
		<dec:AsyncInitDecorator order="2" completeEvent="customComplete" errorEvent="customError"/>
	</flextag:MxmlObjectDefinitionFactoryTag> 
	
	
</mx:Object> 