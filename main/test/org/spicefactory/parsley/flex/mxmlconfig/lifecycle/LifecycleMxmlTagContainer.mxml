<mx:Object 
	xmlns:mx="http://www.adobe.com/2006/mxml"
	xmlns:dec="org.spicefactory.parsley.registry.decorator.*"
	xmlns:flextag="org.spicefactory.parsley.flex.tag.*">
	
	<mx:Script>
		<![CDATA[
			import org.spicefactory.parsley.flex.mxmlconfig.lifecycle.*;		
		]]>
	</mx:Script>
	
	
	<flextag:MxmlObjectDefinitionFactoryTag id="postConstructModel" type="{PostConstructModel}" lazy="true">
		<dec:PostConstructMethodDecorator method="init"/>
	</flextag:MxmlObjectDefinitionFactoryTag> 
	
	<flextag:MxmlObjectDefinitionFactoryTag id="preDestroyModel" type="{PreDestroyModel}" lazy="true">
		<dec:PreDestroyMethodDecorator method="dispose"/>
	</flextag:MxmlObjectDefinitionFactoryTag> 
	
	
</mx:Object> 